{
    "name": "gilbertrabuttsurwa.com__site",
    "version": "1.2.3",
    "description": "Coming Soon",
    "main": "nuxt.config.ts",
    "private": true,
    "scripts": {
        "build": "nuxt build",
        "dev": "nuxt dev --dotenv .env.local",
        "dev-full": "nuxt dev",
        "clean": "nuxi cleanup",
        "deep-clean": "nuxi cleanup && rm package-lock.json & rm -rf node_modules",
        "dev-https": "NODE_TLS_REJECT_UNAUTHORIZED=0 nuxt dev --https --ssl-cert localhost.pem --ssl-key localhost-key.pem --dotenv .env.staging",
        "generate": "nuxt generate",
        "preview": "nuxt preview",
        "postinstall": "nuxt prepare",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "release": "dotenv release-it --",
        "release-minour": "release-it minor"
    },
    "devDependencies": {
        "@commitlint/cli": "^18.6.0",
        "@commitlint/config-conventional": "^18.6.0",
        "@nuxt/devtools": "^1.0.8",
        "@nuxtjs/google-fonts": "^3.1.3",
        "@nuxtjs/supabase": "^1.1.6",
        "@release-it/conventional-changelog": "^8.0.1",
        "@storybook/addon-essentials": "^7.2.1",
        "@storybook/addon-interactions": "^7.2.1",
        "@storybook/addon-links": "^7.2.1",
        "@storybook/addon-mdx-gfm": "^7.2.1",
        "@storybook/blocks": "^7.2.1",
        "@storybook/testing-library": "^0.2.0",
        "@storybook/vue3": "^7.2.1",
        "@storybook/vue3-vite": "^7.2.1",
        "@types/node": "^20.11.16",
        "dotenv-cli": "^7.3.0",
        "nuxt": "^3.10.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "release-it": "^17.0.3",
        "sass": "^1.72.0",
        "sass-loader": "^14.1.1",
        "storybook": "^7.6.12"
    },
    "dependencies": {
        "@nuxtjs/sanity": "^1.8.3",
        "@pinia/nuxt": "^0.5.1",
        "@sanity/client": "^6.12.3",
        "@sanity/image-url": "^1.0.2",
        "@storyblok/nuxt": "^5.6.2",
        "groq": "^3.27.1",
        "highlight.js": "^11.9.0",
        "leaflet": "^1.9.4",
        "mongoose": "^8.1.1",
        "pinia": "^2.1.7",
        "sanity": "^3.27.1",
        "stripe": "^14.22.0",
        "typescript": "^5.3.3",
        "vue3-carousel": "^0.3.1",
        "vue3-marquee": "^4.2.0"
    },
    "release-it": {
        "git": {
            "commitMessage": "chore: release v${version}"
        },
        "github": {
            "release": true
        },
        "npm": {
            "publish": false
        },
        "plugins": {
            "@release-it/conventional-changelog": {
                "infile": "CHANGELOG.md",
                "preset": {
                    "name": "conventionalcommits",
                    "types": [
                        {
                            "type": "feat",
                            "section": "Features"
                        },
                        {
                            "type": "fix",
                            "section": "Bug Fixes"
                        },
                        {}
                    ]
                }
            }
        }
    },
    "overrides": {
        "vue": "latest"
    }
}
