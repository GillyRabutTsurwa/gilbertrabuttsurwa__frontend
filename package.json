{
    "name": "gilbertrabuttsurwa.com__site",
    "version": "1.3.2",
    "description": "Coming Soon",
    "main": "nuxt.config.ts",
    "private": true,
    "scripts": {
        "build": "nuxt build",
        "dev": "nuxt dev --dotenv .env.local",
        "prod-preview": "nuxt dev",
        "clean": "nuxi cleanup",
        "deep-clean": "nuxi cleanup && rm package-lock.json & rm -rf node_modules",
        "dev-https": "NODE_TLS_REJECT_UNAUTHORIZED=0 nuxt dev --https --ssl-cert localhost.pem --ssl-key localhost-key.pem --dotenv .env.local",
        "generate": "nuxt generate",
        "preview": "nuxt preview",
        "postinstall": "nuxt prepare",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "release": "dotenv release-it --",
        "release-minour": "release-it minor"
    },
    "devDependencies": {
        "@chromatic-com/storybook": "^3.2.4",
        "@commitlint/cli": "^19.6.1",
        "@commitlint/config-conventional": "^19.6.0",
        "@nuxt/devtools": "^1.7.0",
        "@nuxtjs/google-fonts": "^3.2.0",
        "@nuxtjs/supabase": "^1.4.6",
        "@release-it/conventional-changelog": "^10.0.0",
        "@sanity/types": "^3.67.1",
        "@storybook/addon-essentials": "^8.5.2",
        "@storybook/addon-interactions": "^8.5.2",
        "@storybook/addon-links": "^8.5.2",
        "@storybook/addon-mdx-gfm": "^8.5.2",
        "@storybook/blocks": "^8.5.2",
        "@storybook/test": "^8.5.2",
        "@storybook/vue3": "^8.5.2",
        "@storybook/vue3-vite": "^8.5.2",
        "@types/node": "^22.13.0",
        "dotenv-cli": "^8.0.0",
        "nuxt": "^3.14.0",
        "release-it": "^18.1.2",
        "sass": "^1.83.4",
        "sass-loader": "^16.0.4",
        "storybook": "^8.5.2"
    },
    "dependencies": {
        "@nuxtjs/sanity": "^1.13.1",
        "@pinia/nuxt": "^0.9.0",
        "@sanity/client": "^6.27.2",
        "@sanity/image-url": "^1.1.0",
        "@vueuse/core": "^12.5.0",
        "groq": "^3.67.1",
        "highlight.js": "^11.11.1",
        "leaflet": "^1.9.4",
        "mongoose": "^8.9.6",
        "pinia": "^2.3.1",
        "sanity": "^3.72.1",
        "stripe": "^17.6.0",
        "typescript": "^5.7.3",
        "vue3-carousel": "^0.13.0",
        "vue3-marquee": "^4.2.2"
    },
    "release-it": {
        "git": {
            "commitMessage": "chore: release v${version}"
        },
        "github": {
            "release": true
        },
        "npm": {
            "publish": false
        },
        "plugins": {
            "@release-it/conventional-changelog": {
                "infile": "CHANGELOG.md",
                "preset": {
                    "name": "conventionalcommits",
                    "types": [
                        {
                            "type": "feat",
                            "section": "Features"
                        },
                        {
                            "type": "fix",
                            "section": "Bug Fixes"
                        },
                        {}
                    ]
                }
            }
        }
    },
    "overrides": {
        "vue": "latest"
    }
}
